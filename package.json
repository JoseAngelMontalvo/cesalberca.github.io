{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://cesalberca.github.io",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "./build.sh deploy",
    "ci": "yarn lint --no-fix && yarn test",
    "start": "vue-cli-service serve",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "core-js": "3.4.1",
    "marked": "0.7.0",
    "register-service-worker": "1.6.2",
    "vue": "2.6.10",
    "vue-analytics": "5.18.0",
    "vue-class-component": "7.1.0",
    "vue-prism-component": "1.1.1",
    "vue-property-decorator": "8.3.0",
    "vue-router": "3.1.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/marked": "0.7.2",
    "@vue/cli-plugin-babel": "4.0.5",
    "@vue/cli-plugin-eslint": "4.0.5",
    "@vue/cli-plugin-pwa": "4.0.5",
    "@vue/cli-plugin-router": "4.0.5",
    "@vue/cli-plugin-typescript": "4.0.5",
    "@vue/cli-plugin-unit-jest": "4.0.5",
    "@vue/cli-service": "4.0.5",
    "@vue/eslint-config-prettier": "5.1.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-plugin-prismjs": "1.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "5.2.3",
    "frontmatter-markdown-loader": "3.1.0",
    "gh-pages": "2.1.1",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "prismjs": "1.17.1",
    "typescript": "3.5.3",
    "vue-template-compiler": "2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "pre-push": "yarn ci"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
